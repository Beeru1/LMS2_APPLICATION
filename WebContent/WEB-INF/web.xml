<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app id="WebApp">
	<display-name>LMS</display-name>

	<filter>
		<filter-name>LoginFilter</filter-name>
		<display-name>LoginFilter</display-name>
		<filter-class>com.ibm.lms.common.LoginFilter</filter-class>
		<init-param>
			<param-name>onError</param-name>
			<param-value>/initLogin.do</param-value>
		</init-param>
	</filter>
	<filter-mapping>
		<filter-name>LoginFilter</filter-name>
		<url-pattern>*.jsp</url-pattern>
	</filter-mapping>

	<listener>
		<listener-class>com.ibm.lms.common.SessionListener</listener-class>
	</listener>

	<servlet>
		<servlet-name>action</servlet-name>
		<servlet-class>org.apache.struts.action.ActionServlet</servlet-class>
		<init-param>
			<param-name>config</param-name>
			<param-value>/WEB-INF/struts-config.xml</param-value>
		</init-param>
		<init-param>
			<param-name>queryConfig</param-name>
			<param-value>/WEB-INF/conf/Query.xml</param-value>
		</init-param>
		<init-param>
			<param-name>debug</param-name>
			<param-value>2</param-value>
		</init-param>
		<init-param>
			<param-name>detail</param-name>
			<param-value>2</param-value>
		</init-param>
		<init-param>
			<param-name>validate</param-name>
			<param-value>true</param-value>
		</init-param>
		<load-on-startup>2</load-on-startup>
	</servlet>

	<servlet>
		<servlet-name>TilesServet</servlet-name>
		<servlet-class>org.apache.struts.tiles.TilesServlet</servlet-class>
		<init-param>
			<param-name>definitions-config</param-name>
			<param-value>/WEB-INF/tiles-defs.xml</param-value>
		</init-param>
		<load-on-startup>3</load-on-startup>
	</servlet>
	<servlet>
		<servlet-name>com_ibm_lms_webservice_UpdateLeadDispositionService</servlet-name>
		<servlet-class>
			com.ibm.lms.webservice.UpdateLeadDispositionService</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>

	<servlet>
		<servlet-name>leadSMS</servlet-name>
		<servlet-class>com.ibm.km.servlet.SmsLeadClosureServlet</servlet-class>
		<load-on-startup>3</load-on-startup>
	</servlet>
	<servlet>
		<servlet-name>com_ibm_lms_webservice_LeadCaptureWebService</servlet-name>
		<servlet-class>
		com.ibm.lms.webservice.LeadCaptureWebService</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet>
		<servlet-name>com_ibm_lms_webservice_PushMstrWebService</servlet-name>
		<servlet-class>
		com.ibm.lms.webservice.PushMstrWebService</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet>
		<servlet-name>com_ibm_lms_webservice_AgencyLeadCaptureWebService</servlet-name>
		<servlet-class>
		com.ibm.lms.webservice.AgencyLeadCaptureWebService</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet>
		<servlet-name>com_ibm_lms_webservice_UpdateLeadWebService</servlet-name>
		<servlet-class>
		com.ibm.lms.webservice.UpdateLeadWebService</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	
	<servlet>
		<servlet-name>com_ibm_lms_webservice_LeadUpdatedDataService</servlet-name>
		<servlet-class>
		com.ibm.lms.webservice.LeadUpdatedDataService</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet>
		<servlet-name>LoadPropertyServlet</servlet-name>
		<display-name>LoadPropertyServlet</display-name>
		<description></description>
		<servlet-class>
		com.ibm.km.servlet.LoadPropertyServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>com_ibm_lms_webservice_GisInfoCaptureWebService</servlet-name>
		<servlet-class>
		com.ibm.lms.webservice.GisInfoCaptureWebService</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet>
		<servlet-name>com_ibm_lms_webservice_LeadCaptureServiceFirstVers</servlet-name>
		<servlet-class>
		com.ibm.lms.webservice.LeadCaptureServiceFirstVers</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet>
		<servlet-name>com_ibm_lms_webservice_IDOCDataStatusService</servlet-name>
		<servlet-class>
		com.ibm.lms.webservice.IDOCDataStatusService</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet>
		<servlet-name>com_ibm_lms_webservice_RetrieveIDOCStatusDataService</servlet-name>
		<servlet-class>
		com.ibm.lms.webservice.RetrieveIDOCStatusDataService</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet>
		<servlet-name>com_ibm_lms_webservice_UpdateLeadDataFirstVers</servlet-name>
		<servlet-class>
		com.ibm.lms.webservice.UpdateLeadDataFirstVers</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet>
		<servlet-name>com_ibm_lms_webservice_RetrieveLeadDataFirstVers</servlet-name>
		<servlet-class>
		com.ibm.lms.webservice.RetrieveLeadDataFirstVers</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet>
		<servlet-name>com_ibm_lms_webservice_RetrieveLeadDataService</servlet-name>
		<servlet-class>
		com.ibm.lms.webservice.RetrieveLeadDataService</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet>
		<servlet-name>ReceiveSMS</servlet-name>
		<display-name>ReceiveSMS</display-name>
		<description></description>
		<servlet-class>
		com.ibm.lms.actions.ReceiveSMS</servlet-class>
	</servlet>
	

	<servlet-mapping>
		<servlet-name>leadSMS</servlet-name>
		<url-pattern>services/leadSms</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>action</servlet-name>
		<url-pattern>*.do</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>com_ibm_lms_webservice_UpdateLeadDispositionService</servlet-name>
		<url-pattern>services/UpdateLeadDispositionService</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>com_ibm_lms_webservice_LeadCaptureWebService</servlet-name>
		<url-pattern>services/LeadCaptureWebService</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>com_ibm_lms_webservice_PushMstrWebService</servlet-name>
		<url-pattern>services/PushMstrWebService</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>com_ibm_lms_webservice_AgencyLeadCaptureWebService</servlet-name>
		<url-pattern>services/AgencyLeadCaptureWebService</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>com_ibm_lms_webservice_UpdateLeadWebService</servlet-name>
		<url-pattern>services/UpdateLeadWebService</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>com_ibm_lms_webservice_LeadUpdatedDataService</servlet-name>
		<url-pattern>services/LeadUpdatedDataService</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LoadPropertyServlet</servlet-name>
		<url-pattern>/LoadCacheServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>com_ibm_lms_webservice_GisInfoCaptureWebService</servlet-name>
		<url-pattern>services/GisInfoCaptureWebService</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>com_ibm_lms_webservice_LeadCaptureServiceFirstVers</servlet-name>
		<url-pattern>services/LeadCaptureServiceFirstVers</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>com_ibm_lms_webservice_IDOCDataStatusService</servlet-name>
		<url-pattern>services/IDOCDataStatusService</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>com_ibm_lms_webservice_RetrieveIDOCStatusDataService</servlet-name>
		<url-pattern>services/RetrieveIDOCStatusDataService</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>com_ibm_lms_webservice_UpdateLeadDataFirstVers</servlet-name>
		<url-pattern>services/UpdateLeadDataFirstVers</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>com_ibm_lms_webservice_RetrieveLeadDataFirstVers</servlet-name>
		<url-pattern>services/RetrieveLeadDataFirstVers</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>com_ibm_lms_webservice_RetrieveLeadDataService</servlet-name>
		<url-pattern>services/RetrieveLeadDataService</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ReceiveSMS</servlet-name>
		<url-pattern>/receiveSMS</url-pattern>
	</servlet-mapping>
	<session-config>
		<session-timeout>30</session-timeout>
	</session-config>
	<mime-mapping>
		<extension>doc</extension>
		<mime-type>application/vnd.ms-word</mime-type>
	</mime-mapping>

	   <welcome-file-list>
		<welcome-file>jsp/redirect.jsp</welcome-file>
	</welcome-file-list>
	<error-page>
		<exception-type>java.lang.Throwable</exception-type>
		<location>/jsp/errors/error404Page.jsp</location>
	</error-page>
	<error-page>
		<error-code>404</error-code>
		<location>/jsp/errors/error404Page.jsp</location>
	</error-page>
	<error-page>
		<error-code>500</error-code>
		<location>/jsp/errors/error500Page.jsp</location>
	</error-page>
	<taglib>
		<taglib-uri>/WEB-INF/struts-bean.tld</taglib-uri>
		<taglib-location>/WEB-INF/struts-bean.tld</taglib-location>
	</taglib>
	<taglib>
		<taglib-uri>/WEB-INF/struts-html.tld</taglib-uri>
		<taglib-location>/WEB-INF/struts-html.tld</taglib-location>
	</taglib>
	<taglib>
		<taglib-uri>/WEB-INF/struts-logic.tld</taglib-uri>
		<taglib-location>/WEB-INF/struts-logic.tld</taglib-location>
	</taglib>
	<taglib>
		<taglib-uri>/WEB-INF/struts-nested.tld</taglib-uri>
		<taglib-location>/WEB-INF/struts-nested.tld</taglib-location>
	</taglib>
	<taglib>
		<taglib-uri>/WEB-INF/struts-template.tld</taglib-uri>
		<taglib-location>/WEB-INF/struts-template.tld</taglib-location>
	</taglib>
	<taglib>
		<taglib-uri>/WEB-INF/struts-tiles.tld</taglib-uri>
		<taglib-location>/WEB-INF/struts-tiles.tld</taglib-location>
	</taglib>
	 <env-entry>
		<env-entry-name>dao/UserDAO</env-entry-name>
		<env-entry-value>com.ibm.appsecure.dao.UserDAOImpl</env-entry-value>
		<env-entry-type>java.lang.String</env-entry-type>
	</env-entry>
</web-app>